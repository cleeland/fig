#!/usr/bin/env ruby

$LOAD_PATH << File.expand_path(File.join(File.dirname(__FILE__), %w< .. lib > ))

require 'rubygems'

require 'fig/command'

# Identical to regular fig, but doesn't use exception handling so you can see
# stack traces.
return_code = Fig::Command.new.run_fig(ARGV)
exit return_code
